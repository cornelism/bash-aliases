alias aliases='subl /home/michael/.bash_aliases'
alias nt='nautilus .'
alias git-rad='git rm $(git ls-files --deleted)'

alias mount_dropbox='sudo mount -t vboxsf dropbox /media/dropbox && dropbox'

alias dropbox='cd /media/dropbox/ && nt'

alias repos='cd /home/michael/repos/'
alias websites='cd /home/michael/websites/'

alias portfolio='cd /home/michael/websites/personal/portfolio/'
alias portfolio_gulp='cd /home/michael/websites/personal/portfolio/etc/gulp/ && gulp'
alias portfolio_theme='cd /home/michael/websites/personal/portfolio/sites/all/themes/portfolio'

alias modules="cd sites/all/modules/"
